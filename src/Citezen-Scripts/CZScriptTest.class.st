{ #category : #tests }
CZScriptTest >> testAuthor [
	"self debug: #testAuthor"
	
	| dict |
	sc keepAuthor: 'Ducasse'.
	self deny: sc bibList isEmpty.
	dict := sc bibList groupedBy: [:each | each type].
	self assert: (dict at: #article) size = 4.
	self assert: (dict at: #book) size = 4.
	self assert: (dict at: #techreport) size = 6
]

{ #category : #tests }
CZScriptTest >> testAuthorBlackListed [
	"self debug: #testAuthorBlackListed"
	
	| dict |
	sc keepAuthor: 'Ducasse'.
	sc blackListedKeys: #(Balm09a).
	sc removeBlackList.
	self deny: sc bibList isEmpty.
	dict := sc bibList groupedBy: [:each | each type].
	self assert: (dict at: #article) size = 4.
	self assert: (dict at: #book) size = 4.
	self assert: (dict at: #inproceedings) size = 22.
	self assert: (dict at: #techreport) size = 5
	
