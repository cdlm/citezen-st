{ #category : #old }
CZScriptTest >> createTestBibFile [
	"self new createTestBibFile"
	(FileDirectory default newFileNamed: self bibFileName)
		nextPutAll: (self scgbibSample copyReplaceAll: String cr with: String lf);
		close
]

